<%= provide(:title, 'All cloud matches') %>
<h1>All cloud matches</h1>

<div>
	<%= link_to "&rsaquo; Add new cloud match".html_safe, new_cloud_match_path %>
</div>

<table class="data" cellpadding="0" cellspacing="0" border="0">
	<tr>
		<th>Match</th>
		<th>Cloud</th>
		<th>Match Type</th>
		<th>Match list</th>
		<th>&nbsp;</th>
	</tr>
	<%= render @cloud_matches %>
</table>