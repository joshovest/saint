<h1>Listing brand_matches</h1>

<table>
  <tr>
    <th>Match list</th>
    <th>Exclude list</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @brand_matches.each do |brand_match| %>
  <tr>
    <td><%= brand_match.match_list %></td>
    <td><%= brand_match.exclude_list %></td>
    <td><%= link_to 'Show', brand_match %></td>
    <td><%= link_to 'Edit', edit_brand_match_path(brand_match) %></td>
    <td><%= link_to 'Destroy', brand_match, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Brand match', new_brand_match_path %>
